{
  "name": "custom-reference-table",
  "version": "0.2.2",
  "private": false,
  "author": "junichi-kose@peer-connect.co.jp",
  "license": "MIT",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode=production",
    "build:dev": "cross-env NODE_ENV=development webpack --mode=development",
    "package": "kintone-plugin-packer --ppk private.ppk --out ./release/plugin.zip plugin",
    "build:package": "npm run build && npm run package",
    "build:dev:package": "npm run build:dev && npm run package",
    "upload": "kintone-plugin-uploader ./release/plugin.zip"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@kintone/dts-gen": "^5.0.21",
    "@kintone/plugin-packer": "^5.0.36",
    "@kintone/plugin-uploader": "^6.0.14",
    "@kintone/webpack-plugin-kintone-plugin": "^5.0.36",
    "@types/luxon": "^2.3.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "babel-loader": "^8.2.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "style-loader": "^3.3.1",
    "tslib": "^2.3.1",
    "typescript": "^4.6.2",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@kintone/kintone-ui-component": "^0.9.0",
    "@kintone/rest-api-client": "^2.0.36",
    "luxon": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
